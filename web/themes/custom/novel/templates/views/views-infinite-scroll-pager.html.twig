{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}
{% if items.next %}
<ul{{ attributes }}>
  <li class="result-pager">
     <p class="text-small-caption result-pager__title">
   {{ options.button_text }}
  </p>
  <a class="btn-primary btn-outline btn-medium arrow__hover--right-small" href="{{ items.next.href }}" title="{{ 'Load more items'|t }}" rel="next">{{ "SHOW MORE"|t }}</a>
  </li>
</ul>
{% endif %}
