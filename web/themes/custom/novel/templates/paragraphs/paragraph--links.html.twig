{% for link in links %}
  <a href="{{ link.href }}" target="{{ link.target }}" rel="{{ link.target == '_blank' ? 'noopener noreferrer' : '' }}"
     class="link-with-icon arrow__hover--right-large">
    <div class="link-with-icon__icon {{ link.linkIconClass }}">
      <img class="invert" src="/{{ baseIconPath }}/{{ link.iconFolder }}/{{ link.iconFile }}.svg" alt="" />
    </div>
    {{ link.linkText }}
    {{ source( baseIconPath ~'/arrow-ui/icon-arrow-ui-large-right.svg') }}
  </a>
{% endfor %}
