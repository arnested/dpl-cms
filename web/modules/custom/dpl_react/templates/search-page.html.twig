<div{{ attributes }}>
  <div class="search-result-page">
    {% if showing_results_for_text and q %}
      <h1 class="text-header-h2 mb-16 search-result-title text-loading" >
        {{ showing_results_for_text }} "{{ q }}"
      </h1>
    {% endif %}
    <div class="ssc mt-48">
      <div class="ssc-head-line"/>
    </div>
    <ul class="search-result-page__list my-32">
      {% for fake_item in search_result_skeleton_item_fakes %}
        <li>{% include 'search-result-item.html.twig' %}</li>
      {% endfor %}
    </ul>
  </div>
</div>
